{
    "background": {
       "scripts": [ "request.js" ]
    },
    "description": "Redirect reddit/hn pages to their 'new | newest' equivalents",
    "icons": {
        "16": "SA-16x16.png",
        "48": "SA-48x48.png",
        "128": "SA-128x128.png"
    },
    "manifest_version": 2,
    "name": "Smile Always",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "http://news.ycombinator.com/",
        "https://news.ycombinator.com/",
        "http://www.reddit.com/*",
        "https://www.reddit.com/*"
    ],
    "version": "0.1",
    "homepage_url": "https://github.com/rgardner/newalways",
    "author": "Robert Gardner",
    "page_action": {
        "default_title" : "SmileAlways is on",
        "default_icon" : "happy-smile.png"
    }
}
